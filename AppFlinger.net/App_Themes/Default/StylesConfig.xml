        <style class="Video.PIP" left="47" top="77" width="160" height="120" padding="rect(5,50,5,5)"/>
      </style>

      <!-- Radio Button Group styles -->
      <style class="RadioButtonGroup" left="0" width="3" background="@Transparent"/>

      <!-- Radio Button styles -->
